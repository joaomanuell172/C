#include <stdio.h> 

int main() {
    
    
    float numeroReal; // Trocamos 'int' por 'float'
    
    printf("Digite um numero real (ex: 7.5): ");
    
    scanf("%f", &numeroReal);
    
    printf("O numero que voce digitou foi: %f\n", numeroReal);
    
    return 0; // 6. Avisar que terminou
}
